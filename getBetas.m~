% plot(betas,meanReturns, 'rx')
% hold on
% [corr,astBeta,astSlope]=regression(betas,meanReturns)
% plot([max(betas),min(betas)],[max(betas)*astBeta+astSlope,min(betas)*astBeta+astSlope])

end 