function betas = getBetas()

load('bbgdata.mat')
rMkt = Returns(:,5)';
vmkt = var(
betas = [];
for i=1:12
    rAst = Returns(:,i)';
    
    cov = cov(rAst,rMkt);
    cov = cov(1,2);
    cov/vmkt
    [corr,astBeta,astSlope]=regression(rMkt,rAst);
    betas = [betas astBeta];
end

